Read	.target.tmpdir "/tmp"
Read	.target.stat "/y2update/control.xml" $[]
Read	.target.stat "/control.xml" $[]
Read	.target.stat "/etc/YaST2/control.xml" $[]
Read	.target.stat "/usr/share/YaST2/control/control.xml" $[]
